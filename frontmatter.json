  "frontMatter.taxonomy.tags": [
  ],
	"frontMatter.data.folders": [
    {
			"id": "Galleries",
			"path": "[[workspace]]/_data/galleries",
			"schema": {
				"type": "object",
				"title": "Galleries",
				"properties": {
					"title": {
						"type": "string",
						"title": "Title"
					},
					"picture_path": {
						"type": "path",
						"title": "Picture Path"
					},
					"directory": {
						"type": "path",
						"title": "Same as Picture Path"
					}
				}
			}
		}
  ],
	"frontMatter.data.files": [
		{
			"id": "overview",
			"title": "Gallery Overview",
			"file": "[[workspace]]/_data/galleries/overview.yml",
			"type": "yaml",
			"schema": {

			}
		}
	],
  "frontMatter.taxonomy.categories": [],
  "frontMatter.content.snippets": {
    "Insert Image": {
      "description": "Image with caption",
      "body": "{{< caption \"[[&mediaUrl]]\" \"[[caption]]\" >}}",
      "isMediaSnippet": true
    }
  }
