    {
      "title": "IMGs",
      "path": "[[workspace]]/assets/img"
    },
    {
      "title": "Includes",
      "path": "[[workspace]]/_includes"
    },
    {
      "title": "Authors",
      "path": "[[workspace]]/_authors"
    },
    {
      "title": "Posts",
      "path": "[[workspace]]/_posts"
    },
    {
